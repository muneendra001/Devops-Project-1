#Jenkins-master group IPs

[jenkins-master]
10.1.1.181

# Jenkins-master variables to connect ansible to the jenkins-master by using ansible_user and ansible_ssh_private_key_file.

[jenkins-master:vars]
ansible_user=ubuntu
ansible_ssh_private_key_file=/opt/dpp.pem


#Jenkins-slave group IPs

[jenkins-slave]
10.1.1.75


# Jenkins-master variables to connect ansible to the jenkins-slave by using ansible_user and ansible_ssh_private_key_file.

[jenkins-slave:vars]
ansible_user=ubuntu
ansible_ssh_private_key_file=/opt/dpp.pem
